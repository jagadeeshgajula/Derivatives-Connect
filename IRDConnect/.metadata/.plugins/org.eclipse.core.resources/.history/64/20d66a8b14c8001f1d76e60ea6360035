#include<iostream>
#include "tradeTicket.h"
using namespace std;
namespace IRDConnect {


int main(int arvc, char * arv[])
{

std::string myName = "Ram";

    cout << "Hello There" << endl;
	TradeTicket tt1;
	tt1.setTradeDate("01/10/2010");
    cout << "TradeDate is:" << tt1.getTradeDate() <<"\n";
    cout <<"Hello Dear Friend!\nHow are you?  \n";


    tt1.setStartDate("03/10/2010");
    cout << "StartDate is:" << tt1.getStartDate() <<"\n";
    cout <<"Hello Dear Friend!\nHow are you? \n";
    cin >> myName;
    cout << myName << endl;
return 1;
}
