#include "amortSchedule.h"

using namespace IRDConnect;

AmortSchedule::AmortSchedule()
{
	// nothing to be done
}

AmortSchedule::AmortSchedule (
					Date startDate,
					Date endDate,
					double amortNotional )
{

	_startDate = startDate;
	_endDate = endDate;
	_amortNotional = amortNotional;


}

AmortSchedule::~AmortSchedule()
{
	// do nothing
}
